  "xo": {
    "extends": "xo-react",
    "esnext": true,
    "semicolon": false,
    "space": 2,
    "globals": [
      "sessionStorage",
      "document"
    ],
    "rules": {
      "curly": [
        "error",
        "multi-line"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "no-return-assign": "off",
      "import/no-unassigned-import": "off",
      "arrow-body-style": [
        "error",
        "as-needed"
      ],
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".jsx"
          ]
        }
      ],
      "react/forbid-component-props": 0,
      "react/forbid-prop-types": 2,
      "react/prefer-stateless-function": 2
    }
  }
}
